/**
 * This function returns a dictionary with the table task variables.
 *
 * The function returns an array with two keys: `table_task` and `variables`.
 * The `table_task` key contains the name of the table task.
 *
 * The `rows` variable is a string that contains the rows of the table task.
 */
function table_task_theme($existing, $type, $theme, $path) {
  // Set the `rows` variable to an empty string.
  $rows = '';

  // Return an array with the `table_task` and `variables` keys.
  return [
    'table_task' => [
      'variables' => [
        'rows' => $rows,
      ],
    ],
  ];
}
